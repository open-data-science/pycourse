{
  "lang": "ru",
  "fileExtensions": [
    ".md",
    ".svg"
  ],
  "excludeFiles": [
    ".git",
    "node_modules",
    ".yaspellerrc.json",
    ".gitignore"
  ],
  "ignoreUppercase": true,
  "ignoreUrls": true,
  "ignoreCapitalization": true,
  "checkYo": false,
  "ignoreText": [
    "Ско бки",
    "\\w[\\$\\_]-й",
    "\\d{1,3}-?[ыйx]",
    "\\d{1,3}х\\d{1,3}",
    "\\sотн.\\s",
    ["– .*\\]", "g"]
  ],
  "dictionary": [
    "Баушенко",
    "Россум",
    "Авито",
    "Сбер",
    "СПбГЭУ",
    "автотест.*",
    "ангармони.*",
    "автоэнкодер.*",
    "баг.*",
    "мультистрочн.*",
    "отрисов.*",
    "переиспользова.*",
    "прототипирован.*",
    "каррировани.*",
    "хеш.*",
    "SVD"
  ]
}
